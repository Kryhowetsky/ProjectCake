@using ProjectCake.Models
@model List<RespondViewModel>




<div class="col-md-12">
    <table>
        @foreach (var comment in Model)
        {
            <tr>
                <th>@comment.Name</th>
                <th>@comment.AddedDate</th>
                <th>@comment.Text</th>
            </tr>

        }
    </table>
</div>